<div class="search-form mx-auto">
  <%= search_form_for @search, url: root_path do |f|%>
    <div>
      <%= f.country_select :country_code_cont, include_blank: true, selected: "", class: "serch-field" %>
    </div>
    <div>
      <%= f.search_field :name_or_content_or_country_code_or_area_cont_any, placeholder: "Serch...", class: "serch-field" %>
    </div>
    <div>
      <%= f.submit "検索" %>
    <div>
  <% end %>
</div>